{
  "name": "officellm",
  "version": "1.0.0",
  "description": "A TypeScript library for multi-model agentic architecture with managers and workers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm",
    "ai",
    "agents",
    "multi-agent",
    "typescript",
    "manager",
    "worker",
    "tools"
  ],
  "author": "Anirudh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anirudhp26/officellm.git"
  },
  "bugs": {
    "url": "https://github.com/anirudhp26/officellm/issues"
  },
  "homepage": "https://github.com/anirudhp26/officellm#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "openai": "^6.8.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
